<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:RealEstateMAUIApp"
             x:Class="RealEstateMAUIApp.Person">
    <VerticalStackLayout Padding="5" Spacing="5">
        <Label Text="Person" x:Name="TitleLabel" FontSize="Small" />
        <Entry x:Name="FirstName" Placeholder="First name"  />
        <Entry x:Name="LastName" Placeholder="Last name" />
        <local:Address x:Name="PersonAddress"  />
    </VerticalStackLayout>
</ContentView>
